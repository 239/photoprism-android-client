<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools" tools:ignore="MissingQuantity">
    <!-- There must be "other" instead of "many",
         otherwise the app crashes in runtime -->

    <plurals name="imported_search_bookmarks">
        <item quantity="one">%1$d marque-page importé</item>
        <item quantity="other">%1$d marque-pages importés</item>
    </plurals>
    <plurals name="years_ago">
        <item quantity="one">Il y a %1$d an</item>
        <item quantity="other">Il y a %1$d ans</item>
    </plurals>
    <plurals name="activated_extensions">
        <item quantity="one">%1$d extension activée</item>
        <item quantity="other">%1$d extensions activées</item>
    </plurals>
    <plurals name="files">
        <item quantity="one">%1$d fichier</item>
        <item quantity="other">%1$d fichiers</item>
    </plurals>
</resources>
